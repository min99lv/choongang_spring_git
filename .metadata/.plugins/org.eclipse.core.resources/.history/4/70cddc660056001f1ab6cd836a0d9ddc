<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.oracle.oBootMybatis01.EmpMapper">
	<select id="empTotal" resultType="int"> <!-- 요청하면 숫자로 돌려줘  -->
	 select Count(*) from emp <!-- 대소문자 구분 X -->
	</select>
	<select id="tkEmpListAll" parameterType="Emp" resultType="Emp">
		select *
		from 
		(
				select rownum rn, a.* 
				from 
				(
					select * from emp order by empno
				) a
		)
		where rn between #{start} and #{end}
	</select>
	<select id="tkEmpSelOne" parameterType="int" resultType="int">
	select *
	from emp
	where empno = #{empno}
	</select>
</mapper>
