package com.oracle.oBootMybatis01.model;


import jakarta.validation.constraints.NotEmpty;
import lombok.Data;

@Data
public class Emp {
	private int empno;
	@NotEmpty
	private String ename;
	private String job;
	private int mgr;
	private String hiredate; // 계속 날짜를 수정해가면서 저장하는 것은 String
	private int sal;
	private int comm;
	private int deptno;
}
