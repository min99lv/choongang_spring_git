<script type="text/javascript">
	function getEmpListUpdate() {
		alert("getEmpListUpdateTest RUN....")
		// group번호 가져오기
		var arr = new Array();
		var item;
		
		<c:forEach items="${listEmp}" var="item">
		arr.push({
			empno:${item.empno}
			,ename:${item.ename}
			,deptno:${item.deptno}
		});
		</c:forEach>
		
		for(var i = 0; i<arr.length){
			alert("arr.empno->"+i+":"+arr[i].empno);
			alert("arr.ename->"+i+":"+arr[i].ename);
			i++;
			if(i>2) return;
		}
	}
</script>
